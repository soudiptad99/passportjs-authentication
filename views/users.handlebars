<div class="row">
	<div class="col-sm-12">
		<h3 class="mb32">List of users</h3>
		<table class="table table-striped table-bordered">
			<thead>
				<tr>
					<th>First Name</th>
					<th>Last Name</th>
					<th>Email</th>
				</tr>	
			</thead>
			<tbody>
				{{#each users}}
				<tr>
					<td>{{local.firstName}}</td>
					<td>{{local.lastName}}</td>
					<td>{{local.email}}</td>
				</tr>
				{{/each}}	
			</tbody>
		</table>
	</div>
</div>